--To re-compile all files:
javac -d . src/*.java
javac -d bin/util src/util/*.java
javac -d bin/communication src/communication/*.java
javac -d bin/replica src/replica/*.java

--To run from command line:
java Master

--To run processes separately from command line
cd bin
java replica.Replica {ID}
java replica.Replica {ID} {ID of replica to write me into the system}
java replica.Client {ID}
